{
	"name": "Youtube Popup Search",
	"version": "0.1",
	"manifest_version": 2,

	"description": "This extension help to you.",

	"permissions":[
		"tabs", "http://*/*"
	],

	"background": {
        "scripts": ["background.js"]
    },

	"browser_action": {
	 	"default_icon": "n0bisuke.jpg",
	 	"default_title": "Search Youtube",
	 	"default_popup": "popup.html"
	}
}